{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "commit": "cz",
    "semantic-release": "semantic-release --branches main"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "name": "next-shield",
  "description": "The shield that every Next.js project needs.",
  "author": "Jorge Acero",
  "module": "dist/next-shield.esm.js",
  "size-limit": [
    {
      "path": "dist/next-shield.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/next-shield.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "husky": "^7.0.1",
    "next": "^11.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-release": "^17.4.4",
    "size-limit": "^5.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imjulianeral/next-shield.git"
  },
  "bugs": {
    "url": "https://github.com/imjulianeral/next-shield/issues"
  },
  "homepage": "https://imjulianeral.github.io/next-shield/",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
